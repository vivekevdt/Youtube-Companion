const express = require('express');

const router = express.Router();


router.get("/get",()=>{
    console.log("get");
})

router.post("/comment/:id",()=>{
    console.log("comment");
})

router.delete("/comment/:id",()=>{
    console.log("delete comment")
})

router.post("/notes",()=>{
    console.log("add notes")
})

module.exports=router